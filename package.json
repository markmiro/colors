{
  "name": "ui-experiments",
  "version": "1.0.0",
  "description": "An experiment for web interfaces, specifically with colors and React",
  "scripts": {
    "test": "ava --watch",
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:non-js": "node build-non-js",
    "build:lib": "babel src/modules --out-dir lib",
    "build": "npm run clean && npm run build:non-js && npm run build:webpack",
    "start": "node dev-server",
    "deploy": "npm run build && node deploy.js",
    "lint": "eslint src"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markmiro/ui-experiments.git"
  },
  "main": "main.js",
  "keywords": [
    "react",
    "reactjs",
    "colors",
    "color",
    "hcl",
    "husl",
    "huslp",
    "chroma",
    "ui"
  ],
  "author": "Mark Miro <contact@markmiro.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/markmiro/ui-experiments/issues"
  },
  "homepage": "https://github.com/markmiro/ui-experiments",
  "devDependencies": {
    "ava": "sindresorhus/ava",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.3.13",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "gh-pages": "^0.9.0",
    "json-loader": "^0.5.2",
    "mkdirp": "^0.5.1",
    "react-transform-catch-errors": "^0.1.1",
    "react-transform-webpack-hmr": "^0.1.2",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^3.1.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "chroma-js": "^1.1.1",
    "d3": "^3.5.6",
    "d3-color": "^0.2.5",
    "d3-scale": "^0.7.0",
    "deep-freeze": "0.0.1",
    "husl": "^6.0.1",
    "radium": "^0.16.6",
    "ramda": "^0.18.0",
    "react": "^0.14.3",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.3",
    "react-fittext": "^0.1.1",
    "react-highlight": "^0.6.1",
    "react-motion": "^0.2.7",
    "redux": "^3.3.1",
    "reset": "^0.1.0",
    "underscore": "^1.8.3"
  }
}
